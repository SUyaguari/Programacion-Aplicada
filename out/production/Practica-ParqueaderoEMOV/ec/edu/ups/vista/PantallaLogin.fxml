<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #fefefe;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.ups.vista.PantallaLogin">
   <children>
      <ImageView fitHeight="277.0" fitWidth="288.0" layoutX="14.0" layoutY="109.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagen/EmovLogo.jpg" />
         </image>
      </ImageView>
      <Text layoutX="344.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Iniciar Sesión">
         <font>
            <Font name="Arial Rounded MT Bold" size="32.0" />
         </font>
      </Text>
      <PasswordField fx:id="txtContraseña" layoutX="313.0" layoutY="221.0" prefHeight="26.0" prefWidth="270.0" promptText="Contraseña" />
      <TextField fx:id="txtCorreo" layoutX="312.0" layoutY="187.0" prefHeight="26.0" prefWidth="270.0" promptText="Correo" />
      <Button fx:id="btnEntrar" layoutX="314.0" layoutY="256.0" mnemonicParsing="false" onAction="#EventInicioSesion" prefHeight="26.0" prefWidth="270.0" text="Entrar" />
      <Button fx:id="btnRegistro" layoutX="455.0" layoutY="339.0" mnemonicParsing="false" onAction="#EventRegistro" text="Click aqui">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Text layoutX="309.0" layoutY="356.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Si usted no esta registrado" />
      <Text layoutX="306.0" layoutY="366.0" strokeType="OUTSIDE" strokeWidth="0.0" text="(Solo para administradores)">
         <font>
            <Font name="Arial Unicode MS" size="8.0" />
         </font>
      </Text>
      <AnchorPane prefHeight="94.0" prefWidth="600.0" style="-fx-background-color: #01acc8;">
         <children>
            <ImageView fitHeight="73.0" fitWidth="200.0" layoutX="429.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagen/LogoCuenca.PNG" />
               </image>
            </ImageView>
            <ImageView fitHeight="44.0" fitWidth="187.0" layoutX="19.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagen/OtroEmovPeroChikitoYAzul.PNG" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
